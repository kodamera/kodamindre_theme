{#
/**
 * @file
 * Theme override for an 'input' of type password form element.
 *
 * @see template_preprocess_input()
 */
#}
{%
  set classes = [
    'c-form-item__input',
    'c-form-item__input--text',
    'c-form-item__input--password'
  ]
%}
<input{{ attributes.addClass(classes) }} />{{ children }}
