{#
/**
 * @file
 * Theme override for an 'input' of type email form element.
 *
 * @see template_preprocess_input()
 */
#}
{%
  set classes = [
    'c-form-item__input',
    'c-form-item__input--text',
    'c-form-item__input--email',
    'bg-gray-100',
    'max-w-full',
    'border border-solid border-gray-300'
  ]
%}
<input{{ attributes.addClass(classes) }} />{{ children }}
