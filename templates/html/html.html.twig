{%
  set body_classes = [
    'w-full',
    'has-no-js',
    node_type ? 'page-node-type--' ~ node_type|clean_class,
    not root_path ? 'is-frontpage',
    logged_in ? 'is-logged-in',
  ]
%}
<!DOCTYPE html>
<html{{ html_attributes }}>
  <head>
    <head-placeholder token="{{ placeholder_token|raw }}">
    <title>{{ head_title|safe_join(' | ') }}</title>
    <css-placeholder token="{{ placeholder_token|raw }}">
    <js-placeholder token="{{ placeholder_token|raw }}">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ directory }}/dist/images/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ directory }}/dist/images/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ directory }}/dist/images/favicons/favicon-16x16.png">
    <link rel="manifest" href="{{ directory }}/dist/images/favicons/site.webmanifest">
    <link rel="mask-icon" href="{{ directory }}/dist/images/favicons/safari-pinned-tab.svg" color="#000000">
    <meta name="msapplication-TileColor" content="#000000">
    <meta name="msapplication-config" content="{{ directory }}/dist/images/favicons/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
  </head>
  <body{{ attributes.addClass(body_classes) }}>
    {#
      Keyboard navigation/accessibility link to main content section in
      page.html.twig.
    #}
    <a href="#main-content" class="sr-only focus:not-sr-only">
      {{ 'Skip to main content'|t }}
    </a>
    {{ page_top }}
    {{ page }}
    {{ page_bottom }}
    <js-bottom-placeholder token="{{ placeholder_token|raw }}">
  </body>
</html>
