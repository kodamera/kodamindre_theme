<div{{ attributes }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes.addClass('block cursor-pointer text-gray-700 font-bold text-sm mb-1') }}>
      {{ 'Search for content'|t }}
    </h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</div>
